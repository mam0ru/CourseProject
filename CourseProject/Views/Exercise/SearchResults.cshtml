@model IEnumerable<CourseProject.Models.Exercise>
@{
}
<br>
@if (Model.Count() != 0)
{
    foreach (var exersise in Model)
    {
        if (exersise != null)
        {
            @Html.ActionLink(exersise.Name, "ShowExercise", "Exercise", new { id = exersise.Id },null)
            <br>
        }
    }
}
else
{
    @Resources.Resource.NotFound
}
