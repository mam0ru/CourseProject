@model CourseProject.ViewModels.SearchReturnModel
@{
}
<br>
@if (Model != null)
{
    if (Model.Users != null)
    {
        foreach (var user in Model.Users)
        {
            if (user != null)
            {
                @Html.ActionLink(user.UserName, "ShowProfile", "Profile", new { id = user.Id }, null)
                <br>
            }
        }
    }
    if (Model.Exercises != null)
    {
        foreach (var exersise in Model.Exercises)
        {
            if (exersise != null)
            {
                 @Html.ActionLink(exersise.Name, "ShowExercise", "Exercise", new {id = exersise.Id}, null)
                 <br>
             }
         }
     }
}
else
{
    @Resources.Resource.NotFound
}
