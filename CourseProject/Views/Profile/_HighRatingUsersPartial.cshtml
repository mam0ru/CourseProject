@model List<CourseProject.Models.ApplicationUser>
@{

}
<h3 style="color:#87CEFA">@Resources.Resource.HighRatingUsers</h3>
<div>
    @for (int i=0; i< Model.Count() && i<5; i++)
    {
        <span class="badge">@(i+1) </span>
        @Html.ActionLink(Model[i].UserName, "ShowProfile", "Profile", new { id = Model[i].Id }, null)
        <br>
    }
</div>


