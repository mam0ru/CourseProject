@model System.Linq.IOrderedEnumerable<CourseProject.Models.ApplicationUser>
@{
    ViewBag.Title = "Rating";
}.

<div class="jumbotron">
    <div class="container">
        <br />
        <table class="table table-striped table-hover ">
            @foreach (var user in Model)
            {
            <tr>
                <td>@Html.ActionLink(user.UserName, "ShowProfile", "Profile", new { id = user.Id}, null)</td>
            </tr>
            }
        </table>
    </div>
</div>
